    "dev:prod": "cross-env NODE_ENV=development SERVER_ENV='prod' nodemon server/index.js --watch server",
    "cgh": "nuxt --hostname 0.0.0.0 --port 3001",
    "build:dev": "cross-env NODE_ENV=production SERVER_ENV=dev nuxt build",
    "build:test": "cross-env NODE_ENV=production SERVER_ENV=test nuxt build",
    "build:prod": "cross-env NODE_ENV=production SERVER_ENV=prod nuxt build",
